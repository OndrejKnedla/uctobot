{
  "version": 2,
  "builds": [
    {
      "src": "ucetni-whatsapp-bot/api/*.py",
      "use": "@vercel/python",
      "config": { 
        "pythonVersion": "3.9",
        "includeFiles": "ucetni-whatsapp-bot/static/**"
      }
    }
  ],
  "routes": [
    {
      "src": "/webhook",
      "dest": "ucetni-whatsapp-bot/api/webhook.py"
    },
    {
      "src": "/(.*)",
      "dest": "ucetni-whatsapp-bot/api/index.py"
    }
  ]
}
